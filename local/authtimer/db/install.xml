<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/authtimer/db" VERSION="20210222" COMMENT="XMLDB file for Moodle local/authtimer"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="local_authtimer" COMMENT="Default comment for local_worktimer">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="authcode" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="user" UNIQUE="true" FIELDS="userid"/>
                <INDEX NAME="usercode" UNIQUE="true" FIELDS="userid, authcode"/>
            </INDEXES>
        </TABLE>

    </TABLES>
</XMLDB>