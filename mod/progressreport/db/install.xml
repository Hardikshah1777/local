      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="fkprogressreportid" TYPE="foreign" FIELDS="progressreportid" REFTABLE="progressreport" REFFIELDS="id"/>
